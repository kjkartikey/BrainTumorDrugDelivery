FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      lfc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [-1 1 1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    front
    {
      type              zeroGradient;
    }

    back
    {
      type              zeroGradient;
    }
}


